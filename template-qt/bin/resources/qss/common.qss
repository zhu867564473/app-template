.SettingWidget {
    background: white;
}

.WidgetsShower {
    background: white;
}

.CentralWidget {
    background: #80848f;
}

QWidget {
    color: #495060;
}

QWidget:focus {
    outline: none; /* 去掉得到焦点时的虚线框 */
}

QLineEdit {
    height: 24px;
    font-size: 12px;
    border: 1px solid #dddee1;
    border-radius: 4px;
    color: #495060;
    background-color: #fff;
    padding: 0 4px;
}

QLineEdit:focus {
    border-color: #57a3f3;
}
